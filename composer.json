{
    "name": "bktz/laravel4-angularjs-starter-site",
    "description": "Laravel 4 AngularJS Starter Site",
    "keywords": ["laravel", "angularjs", "starter"],
	"license": "MIT",
	"authors": [
		{
			"name": "Benjamin Katznelson",
			"homepage": "http://ca.linkedin.com/in/benikatznelson",
			"role": "Developer"
		}
	],
	"require": {
		"laravel/framework": "4.0.*",
		"laravelbook/ardent": "2.1.x",
		"zizaco/entrust": "dev-master",
		"robclancy/presenter": "1.0.*",
		"j20/php-uuid": "dev-master"
	},
	"require-dev": {
		"way/generators": "dev-master",
		"mockery/mockery": "dev-master@dev",
		"summerstreet/woodling": "0.1.*",
		"barryvdh/laravel-ide-helper": "dev-master",
		"leafo/lessphp": "v0.4.0",
		"natxet/CssMin": "dev-master",
		"lmammino/jsmin4assetic": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
